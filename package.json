{
  "name": "typeofweb-discord-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "dev": "tsc-watch --onSuccess \"node ./dist/src/index.js\"",
    "tslint": "tslint -p tsconfig.json",
    "tsc": "tsc --noEmit -p tsconfig.json",
    "lint": "npm run tsc && npm run tslint",
    "test": "npm run lint && npm run test:unit",
    "test:ci": "npm run lint && npm run coverage",
    "test:unit": "npm run mocha -- --file test/before-each.spec.ts 'src/**/*.spec.ts'",
    "coverage": "nyc npm run test:unit",
    "mocha": "mocha",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && rsync -av --exclude='*.ts' src/** dist/src/"
  },
  "keywords": [],
  "author": "Micha≈Ç Miszczyszyn <michal@mmiszy.pl> (https://typeofweb.com/)",
  "license": "ISC",
  "dependencies": {
    "bufferutil": "4.0.3",
    "discord.js": "11.6.4",
    "discord.rss": "5.0.6",
    "dotenv": "8.2.0",
    "erlpack": "github:discordapp/erlpack",
    "isolated-vm": "4.0.0",
    "node-cache": "5.1.2",
    "node-fetch": "2.6.1",
    "polish-plurals": "1.1.0",
    "request": "2.88.2",
    "request-promise": "4.2.5",
    "secure-json-parse": "2.4.0",
    "typescript": "4.2.4",
    "zlib-sync": "0.1.7"
  },
  "devDependencies": {
    "@types/chai": "4.2.16",
    "@types/chai-as-promised": "7.1.3",
    "@types/dotenv": "8.2.0",
    "@types/mocha": "8.2.2",
    "@types/nock": "11.1.0",
    "@types/node": "12.20.10",
    "@types/node-fetch": "2.5.10",
    "@types/secure-json-parse": "1.0.2",
    "@types/sinon": "10.0.0",
    "@types/sinon-chai": "3.2.5",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "husky": "4.2.5",
    "mocha": "8.3.2",
    "nock": "13.0.11",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "sinon": "10.0.0",
    "sinon-chai": "3.6.0",
    "source-map-support": "0.5.19",
    "ts-node": "9.1.1",
    "tsc-watch": "4.2.9",
    "tslint": "6.1.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
