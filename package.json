{
  "name": "typeofweb-discord-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "dev": "tsc-watch --onSuccess \"node ./dist/src/index.js\"",
    "tslint": "tslint -p tsconfig.json",
    "tsc": "tsc --noEmit -p tsconfig.json",
    "lint": "npm run tsc && npm run tslint",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "npm run mocha -- 'src/**/*.spec.ts'",
    "coverage": "nyc npm run test:unit",
    "mocha": "mocha",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && rsync -av --exclude='*.ts' src/** dist/src/"
  },
  "keywords": [],
  "author": "Micha≈Ç Miszczyszyn <michal@mmiszy.pl> (https://typeofweb.com/)",
  "license": "ISC",
  "dependencies": {
    "bufferutil": "4.0.1",
    "discord.js": "11.5.1",
    "discord.rss": "5.0.3",
    "dotenv": "8.2.0",
    "erlpack": "github:discordapp/erlpack",
    "isolated-vm": "3.1.0",
    "node-fetch": "2.6.0",
    "polish-plurals": "1.1.0",
    "typescript": "3.7.4",
    "zlib-sync": "0.1.6"
  },
  "devDependencies": {
    "@types/chai": "4.2.7",
    "@types/chai-as-promised": "7.1.2",
    "@types/dotenv": "8.2.0",
    "@types/mocha": "5.2.7",
    "@types/node": "13.1.2",
    "@types/node-fetch": "2.5.4",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "husky": "3.1.0",
    "mocha": "7.0.0",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "source-map-support": "0.5.16",
    "ts-node": "8.5.4",
    "tsc-watch": "4.0.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tslint && pretty-quick --staged"
    }
  }
}
