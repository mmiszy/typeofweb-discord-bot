{
  "name": "typeofweb-discord-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "dev": "tsc-watch --onSuccess \"node ./dist/src/index.js\"",
    "tslint": "tslint -p tsconfig.json",
    "tsc": "tsc --noEmit -p tsconfig.json",
    "test": "npm run tsc && npm run tslint",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && rsync -av --exclude='*.ts' src/** dist/src/"
  },
  "keywords": [],
  "author": "Micha≈Ç Miszczyszyn <michal@mmiszy.pl> (https://typeofweb.com/)",
  "license": "ISC",
  "dependencies": {
    "bufferutil": "4.0.1",
    "discord.js": "11.5.1",
    "discord.rss": "4.0.2",
    "dotenv": "8.0.0",
    "erlpack": "github:discordapp/erlpack",
    "node-fetch": "2.6.0",
    "polish-plurals": "1.1.0",
    "zlib-sync": "0.1.4"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/node": "12.0.4",
    "@types/node-fetch": "2.3.4",
    "husky": "2.3.0",
    "prettier": "1.17.1",
    "pretty-quick": "1.11.0",
    "tsc-watch": "2.2.1",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "5.5.2",
    "typescript": "3.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tslint && pretty-quick --staged"
    }
  }
}
